; ModuleID = 'LFortran'
source_filename = "LFortran"

@x = external global i64

define i64 @f() {
.entry:
  %f = alloca i64
  store i64 6, i64* @x
  %0 = load i64, i64* @x
  %1 = add i64 %0, 1
  store i64 %1, i64* @x
  %2 = load i64, i64* @x
  %3 = mul i64 3, %2
  store i64 %3, i64* %f
  %4 = load i64, i64* %f
  ret i64 %4
}

