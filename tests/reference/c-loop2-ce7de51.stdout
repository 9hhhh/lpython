#include <assert.h>
#include <complex.h>
#include <inttypes.h>
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <stdbool.h>

#include <lfortran_intrinsics.h>

// Forward declarations
void _lpython_main_program();
void test_for();
void _xx_lcompilers_changed_exit_xx(int32_t error_code);

// Implementations
void _lpython_main_program()
{
    test_for();
}

void test_for()
{
    int32_t i;
    for (i=0; i<=10 - 1; i++) {
        if (i == 0) {
            continue;
        }
        if (i > 5) {
            break;
        }
        if (i == 3) {
            exit(0);
        }
    }
    _xx_lcompilers_changed_exit_xx(2);
}

void _xx_lcompilers_changed_exit_xx(int32_t error_code)
{
    exit(error_code);
}

int main(int argc, char* argv[])
{
    _lpython_main_program();
    return 0;
}
