#include <iostream>
#include <Kokkos_Core.hpp>

int main(int argc, char* argv[])
{
    Kokkos::initialize(argc, argv);
    {
        int i;
        float r;
        r = 1.5;
        std::cout << r << std::endl;
        i = (int)r;
        std::cout << i << std::endl;
    }
    Kokkos::finalize();
    return 0;
}
