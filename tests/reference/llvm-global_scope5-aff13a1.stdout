; ModuleID = 'LFortran'
source_filename = "LFortran"

@x = external global i64

define i64 @f() {
.entry:
  %f = alloca i64
  store i64 6, i64* @x
  %0 = load i64, i64* @x
  %1 = mul i64 2, %0
  store i64 %1, i64* %f
  %2 = load i64, i64* @x
  %3 = add i64 %2, 1
  store i64 %3, i64* @x
  %4 = load i64, i64* @x
  %5 = mul i64 3, %4
  store i64 %5, i64* %f
  %6 = load i64, i64* %f
  ret i64 %6
}

